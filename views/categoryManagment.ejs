              <tbody>

                <% categories.forEach((item,index)=>{ %>
  
                    <tr>
                        <th><%= index+1  %> </th> 
                       
                        <th style="text-transform:capitalize;"><%= item.category  %> </th>

                        <th>
                            <%if(item.status =='available'){%>

                              <form method="post" action="/admin/unlist-category/<%=item._id%>" >
                                <button type="submit" style="background-color: rgb(239, 94, 92); color: white; padding: 10px 40px; font-size: 18px; font-weight: 300;"  onclick="return confirm('are you sure you want to unlist')">unlist</button>
                              
                              </form>
                
                               
                             
                             
                             <%} else{ %>
                
                            
                
                            <form method="post" action="/admin/list-category/<%=item._id%>" >
                              <button type="submit" style="background-color: rgb(126, 204, 135); color: white; padding: 10px 52px; font-size: 18px; font-weight: 300;"  onclick="return confirm('are you sure you want to list')">list</button>
                            
                            </form>
                           
                           
                           <%}%>
                        
                        </form>
