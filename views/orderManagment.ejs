              <span class="mdi mdi-format-line-spacing"></span>
            </button>
          </div>
        </nav>
        <div class="main-panel">
          <div class="content-wrapper">          
            
            <table class="table table-striped table-hover">
              <thead>
                <tr>
                  
                  <th scope="col">No</th>
                  <th scope="col">Name</th>
                  <th scope="col">Client</th>
                  <th scope="col">Payment Status</th>
                  <th scope="col">status</th>
                  <th scope="col">orderId</th>
                  <th scope="col">Total</th>
                  <th scope="col">Details</th>
                </tr>
              </thead>
              <tbody>

                <% order.forEach((item,index)=>{ %>

                  <tr>
                      
                      <td ><%= index+1 %></td>
                      <td ><%= item.orderItems.name %></td>
                      <td ><%= item.address.name %></td>

                      <td class="paid <%= item.paid  %>">
                        <% if(item.paid || item.status == "delivered"){ %> 
                             <p style="color: #55b871;">Paid</p>	
                        <% }else { %> 
                             <p style="color: #cc5858;">Not Paid</p>
                        <% } %>

                      </td>

                      <td><%= item.status%></td>
                      <td><%= item._id %></td>
                      <td><%= item.totalPrice %> </td>
                           
                        
                 
                     <td><a href="/admin/admin-view-order/<%=item._id %>"><button
                          class="btn btn-secondary"
                          type="button"
                          id="dropdownMenuButton"
                          data-toggle="dropdown"
                          aria-haspopup="true"
                          aria-expanded="false"> view</button></a></td>
                        
                        
                        

                          <%} ) %>
              </tbody>
            </table>
            
            
